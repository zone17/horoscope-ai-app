# Backend production environment variables

# Redis configuration
UPSTASH_REDIS_REST_URL=your-redis-url-here
UPSTASH_REDIS_REST_TOKEN=your-redis-token-here

# Feature flags
FEATURE_FLAG_USE_REDIS_CACHE=true
FEATURE_FLAG_USE_RATE_LIMITING=true
FEATURE_FLAG_USE_TIMEZONE_CONTENT=true
FEATURE_FLAG_USE_SCHEMA_MARKUP=false
FEATURE_FLAG_USE_CORE_WEB_VITALS_OPT=false

# OpenAI
OPENAI_API_KEY=your-openai-key-here

# Security
CRON_SECRET=your-cron-secret-here

# Rate limiting
RATE_LIMIT_REQUESTS=20
RATE_LIMIT_DURATION=60 